{
  "name": "rollup-plugin-replace-style-inject",
  "version": "1.0.5",
  "description": "This plugin changes the path to style-inject and installs a built copy of it",
  "main": "src/index.js",
  "scripts": {
    "build": "npx rollup --config rollup.config.js",
    "clean": "npx rimraf dist/**/*",
    "auth": "npm login",
    "upload": "npm publish dist/ --registry",
    "lint": "eslint . --fix --ext .js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZedByl/rollup-plugin-replace-style-inject.git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "eslint": "8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "rimraf": "^3.0.0",
    "rollup": "^2.77.0",
    "rollup-plugin-copy": "^3.4.0",
    "prettier": "^3.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.2"
  },
  "files": [
    "dist",
    "index.d.ts",
    "readme.md"
  ],
  "keywords": [
    "rollup",
    "rollup-plugin",
    "style-inject",
    "replace-style-inject"
  ],
  "license": "MIT",
  "author": "https://github.com/ZedByl",
  "bugs": {
    "url": "https://github.com/ZedByl/rollup-plugin-replace-style-inject/issues"
  },
  "homepage": "https://github.com/ZedByl/rollup-plugin-replace-style-inject#readme"
}
